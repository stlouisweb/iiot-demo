  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "node-sass": "^4.5.0",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
